<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:background="@drawable/agency_card_01"
    android:layout_height="wrap_content"
    android:paddingBottom="20dp"
    android:id="@+id/item_back">


    <ImageView
        android:background="@drawable/logo_radius"
        android:id="@+id/comp_logo"
        android:layout_width="58dp"
        android:layout_height="58dp"
        android:layout_marginLeft="18dp"
        android:layout_marginTop="31dp"
        android:scaleType="fitXY"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="31dp"
        android:text="애드캡슐소프트"
        android:id="@+id/comp_name"
        android:textSize="17dp"
        android:lineHeight="20dp"
        android:letterSpacing="0.01"
        android:lineSpacingExtra="3dp"
        android:textColor="#000000"
        app:layout_constraintLeft_toRightOf="@+id/comp_logo"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/comp_name"
        android:layout_marginTop="10dp"

        app:layout_constraintLeft_toLeftOf="@+id/comp_name"
        android:id="@+id/sticker_01"
        />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/comp_name"
        android:layout_marginTop="10dp"

        app:layout_constraintLeft_toRightOf="@+id/sticker_01"
        android:layout_marginLeft="10dp"
        android:id="@+id/sticker_02"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="#웹 에이전시"
        android:textSize="9dp"
        android:id="@+id/hash_1"
        android:textColor="#50000000"
        android:lineHeight="15dp"
        android:letterSpacing="0.01"
        android:lineSpacingExtra="4dp"
        app:layout_constraintTop_toBottomOf="@+id/sticker_01"
        app:layout_constraintLeft_toLeftOf="@+id/comp_name"
        android:layout_marginTop="10dp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="#웹 에이전시"
        android:textSize="9dp"
        android:id="@+id/hash_2"
        android:textColor="#50000000"
        android:lineHeight="15dp"
        android:letterSpacing="0.01"
        android:lineSpacingExtra="4dp"
        app:layout_constraintTop_toBottomOf="@+id/sticker_01"
        app:layout_constraintLeft_toRightOf="@+id/hash_1"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="#웹 에이전시"
        android:textSize="9dp"
        android:id="@+id/hash_3"
        android:textColor="#50000000"
        android:lineHeight="15dp"
        android:letterSpacing="0.01"
        android:lineSpacingExtra="4dp"
        app:layout_constraintTop_toBottomOf="@+id/sticker_01"
        app:layout_constraintLeft_toRightOf="@+id/hash_2"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="#웹 에이전시"
        android:textSize="9dp"
        android:id="@+id/hash_4"
        android:textColor="#50000000"
        android:lineHeight="15dp"
        android:letterSpacing="0.01"
        android:lineSpacingExtra="4dp"
        app:layout_constraintTop_toBottomOf="@+id/hash_1"
        app:layout_constraintLeft_toLeftOf="@+id/hash_1"
        android:layout_marginTop="2dp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="#웹 에이전시"
        android:textSize="9dp"
        android:id="@+id/hash_5"
        android:textColor="#50000000"
        android:lineHeight="15dp"
        android:letterSpacing="0.01"
        android:lineSpacingExtra="4dp"
        app:layout_constraintTop_toBottomOf="@+id/hash_1"
        app:layout_constraintLeft_toRightOf="@id/hash_4"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="2dp"
        />



    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="137dp" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@+id/guideline"

        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/guideline2"



        android:id="@+id/alphaBtn"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="주요 파트너 보기"
            android:textSize="12dp"
            android:lineSpacingExtra="3dp"
            android:letterSpacing="0.01"
            android:textColor="#000000"
            android:lineHeight="15dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:id="@+id/alphaNmae"
            />
        <ImageView
            android:layout_width="16dp"
            android:layout_height="16dp"
            android:src="@drawable/unfold_btn"
            android:scaleType="fitXY"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/alphaNmae"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginLeft="7dp"
            />







    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="28dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/alphaBtn"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginTop="12dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="10dp"
        android:id="@+id/partner"
        android:visibility="gone"
        >

        <ImageView
            android:layout_width="0dp"
            android:layout_height="28dp"
            android:src="@drawable/client_01"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/img_client02"
            android:scaleType="fitXY"
            android:id="@+id/img_client01"/>
        <ImageView
            android:layout_width="0dp"
            android:layout_height="28dp"
            android:id="@+id/img_client02"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/img_client01"
            app:layout_constraintRight_toLeftOf="@id/img_client03"
            android:scaleType="fitXY"
            android:src="@drawable/client_02"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            />
        <ImageView
            android:layout_width="0dp"
            android:layout_height="28dp"
            app:layout_constraintTop_toTopOf="parent"
            android:id="@+id/img_client03"
            app:layout_constraintLeft_toRightOf="@id/img_client02"
            android:scaleType="fitXY"
            app:layout_constraintRight_toRightOf="parent"
            android:src="@drawable/client_03"
            />

    </androidx.constraintlayout.widget.ConstraintLayout>


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="183dp" />


</androidx.constraintlayout.widget.ConstraintLayout>